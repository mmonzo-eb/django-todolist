[run]
source =
    todolist_app

omit =
    env/*
    templates/*
    todolist_app/migrations/*
    todolist_app/urls
    todolist_app/apps
    todolist_app/admin
    */test*

[report]
exclude_lines =
    pragma: no cover
    def setUp
    def test_
    return raw_input
    print
    if __name__ == '__main__':
    def suite